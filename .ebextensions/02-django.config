container_commands:
  01_migrate:
    command: "django-admin migrate"
    leader_only: true
  02_createsu:
    command: "django-admin createsu"
  03_collectstatic:
    command: "django-admin collectstatic --noinput"
  04_seed_books:
    command: "django-admin seed_books --total=4"
  05_seed_movies:
    command: "django-admin seed_movies --total=4"
  06_seed_reviews:
    command: "django-admin seed_reviews --total=4"
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config/wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings